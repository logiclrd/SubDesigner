<UserControl x:Class="SubDesigner.StampCollectionButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Width="100"
             Height="80">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <Ellipse Grid.Row="0" Grid.Column="0" Width="15" Height="15" Fill="Black" />
        <Ellipse Grid.Row="1" Grid.Column="0" Width="15" Height="15" Fill="Black" />
        <Ellipse Grid.Row="2" Grid.Column="0" Width="15" Height="15" Fill="Black" />
        <Ellipse Grid.Row="3" Grid.Column="0" Width="15" Height="15" Fill="Black" />

        <Rectangle Grid.Row="0" Grid.Column="1" Margin="6" Height="8" RadiusX="4" RadiusY="4" Fill="Black" />
        <Rectangle Grid.Row="1" Grid.Column="1" Margin="6" Height="8" RadiusX="4" RadiusY="4" Fill="Black" />
        <Rectangle Grid.Row="2" Grid.Column="1" Margin="6" Height="8" RadiusX="4" RadiusY="4" Fill="Black" />
        <Rectangle Grid.Row="3" Grid.Column="1" Margin="6" Height="8" RadiusX="4" RadiusY="4" Fill="Black" />
    </Grid>
</UserControl>
